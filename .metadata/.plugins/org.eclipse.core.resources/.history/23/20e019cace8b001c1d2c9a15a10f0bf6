package pack02.subPack;

public class Phone {
	int myNumber;
	int yourNumber;
	boolean call;
	boolean onOff;
	
	public String call(int myNumber) {
		System.out.println(myNumber+"시에 tv를 킴");
		
		return "OK";
	}
	public String call(int myNumber,int yourNumber) {
		System.out.println(myNumber+"시에 tv를 킴");
		
		return "OK";
	}
	public String toString() {	
		return "내 폰번호:"+myNumber+"상대방 폰번호:"+yourNumber+","+onOff;
	}

}
